name: Eggshell Dark
settings:
  - settings:
      scope: eggshell
      caret: '#ff0000'
      lineHighlight: '#F92672'
      background: '#112233'
      foreground: '#ffffff'
      gutter: '#001122'
      gutterForeground: '#ffeeff'

  - name: 'Comment'
    scope: comment
    settings:
      foreground: '#665566'

  - name: 'Invalid'
    scope: invalid
    settings:
      background: '#ff0000'
      foreground: '#ffffff'

  - name: 'Code'
    scope: markup.raw.block
    settings:
      foreground: '#eeeeee'
      background: '#332033'
  - name: 'Code (Inline)'
    scope: markup.raw.code
    settings:
      foreground: '#cccccc'
      background: '#332033'
      fontStyle: bold

  - name: 'Operator'
    scope: support.operator
    settings:
      foreground: '#ffcc00'

  - name: 'String'
    scope: string, text.html
    settings:
      foreground: '#eeeeee'
  - name: 'String Quoted'
    scope: string.quoted, meta.function.interpol
    settings:
      foreground: '#ffcc99'

  - name: 'Constants'
    scope: constant
    settings:
      foreground: '#00ccff'

  - name: 'Variables'
    scope: variable
    settings:
      foreground: '#E7352B'
  - name: 'Variable Parameters'
    scope: variable.parameter
    settings:
      foreground: '#E7352B'

  - name: 'Macro'
    scope: meta.function, meta.class, support.class
    settings:
      foreground: '#ffccff'
  - name: 'Macro/Function/Block Name'
    scope: entity.name, keyword
    settings:
      foreground: '#AE81FF'

  - name: 'Hash'
    scope: map.eggshell
    settings:
      foreground: '#ffff00'
  - name: 'Hash Key'
    scope: constant.character
    settings:
      foreground: '#cc99cc'

  # - name: 'Interpolation'
  #   scope: support.function.interpol
  #   settings:
  #     background: '#ffccff'
  #     color: '#334433'

  - name: 'HTML/Block Tag'
    scope: entity.name.tag, meta.tag
    settings:
      foreground: '#AE81FF'
      #background: '#333333'
  - name: 'HTML Attribute'
    scope: entity.other.attribute-name
    settings:
      foreground: '#ffccff'
  - name: 'HTML Attribute'
    scope: entity.other
    settings:
      foreground: '#ccffaa'
      background: '#333333'

  - name: 'Markup Bold'
    scope: markup.bold
    settings:
      foreground: '#ffcc00'
      background: '#33333a'
      fontStyle: bold
  - name: 'Markup Italic'
    scope: markup.italic
    settings:
      foreground: '#ffcc00'
      background: '#33333a'
      fontStyle: italic
  - name: 'Markup Other'
    scope: markup.other
    settings:
      foreground: '#ffff00'
      background: '#33333a'
  - name: 'Markup Other (HTML block)'
    scope: markup.other.block
    settings:
      foreground: '#bbaabb'
